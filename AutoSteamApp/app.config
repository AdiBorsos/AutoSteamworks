<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- Value in ms (1000ms = 1second) between the pushes of buttons-->
    <add key="DelayBetweenKeys" value="500"/>
    <!-- Value in ms (1000ms = 1second) between next read -->
    <add key="DelayBetweenCombo" value="500"/>
    <!-- Value can be True for AZERTY(QZD) keyboard or False for QUERTY(AWD) keyboard -->
    <add key="IsAzerty" value="false"/>
    <!-- Value can be True for logging everything, False for logging only errors -->
    <add key="IsLogEnabled" value="true"/>

    <!--Use the link below to find the key codes you want to use-->
    <!--Key Code Mapping = https://www.cambiaresearch.com/articles/15/javascript-char-codes-key-codes -->

    <!-- 45 = insert-->
    <add key="keyCodeStart" value="36" />
    <!-- 27 = escape-->
    <add key="keyCodeStop" value="35" />
  </appSettings>
</configuration>